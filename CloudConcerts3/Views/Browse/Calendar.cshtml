@model IEnumerable<CloudConcerts3.Models.Concert>

@{
    ViewBag.Title = "Calendar";
}

<h2>Concerts Calendar</h2>

<div id="toolbar" class="ui-widget-header ui-corner-all" style="padding:3px; vertical-align: middle;">
    <button id="BtnPreviousMonth">Previous Month</button>
    <button id="BtnNextMonth">Next Month</button>
    <span style="white-space:nowrap;">Date: <input type="text" id="dateSelect" size="20" /></span>
</div>

<div id="mycal"></div>

<div id="display-event-form" title="View Agenda Item"></div>

<script>
    var jfcalplugin = $("#mycal").data("plugin");
    jfcalplugin.addAgendaItem(
	"#mycal",
	"Christmas DAY",
	new Date(2015, 1, 25, 20, 0, 0, 0),
	new Date(2015, 1, 25, 23, 59, 59, 999),
	false,
	{
	    fname: "Santa",
	    lname: "Claus",
	    leadReindeer: "Rudolph",
	    myExampleDate: new Date()
	},
	{
	    backgroundColor: "#FF0000",
	    foregroundColor: "#FFFFFF"
	}
    );
</script>
